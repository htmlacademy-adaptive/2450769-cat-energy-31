{% macro contacts() %}
<aside class="contacts">
	<div class="contacts__title">Приглашаем к сотрудничеству дилеров!</div>
	<div class="contacts__adress">ул. Большая Конюшенная, д. 19/8 Санкт-Петербург</div>
	<picture>
		<source type="image/avif" media="(width >= 1280px)" srcset="{{ project.root }}/images/map-1280@1x.avif 1x, {{ project.root }}/images/map-1280@2x.avif 2x" width="1440" height="400">
		<source type="image/avif" media="(width >= 768px)" srcset="{{ project.root }}/images/map-768@1x.avif 1x, {{ project.root }}/images/map-768@2x.avif 2x" width="768" height="400">
		<source type="image/avif" srcset="{{ project.root }}/images/map@1x.avif 1x, {{ project.root }}/images/map@2x.avif 2x" width="320" height="362">
		<source type="image/webp" media="(width >= 1280px)" srcset="{{ project.root }}/images/map-1280@1x.webp 1x, {{ project.root }}/images/map-1280@2x.webp 2x" width="1440" height="400">
		<source type="image/webp" media="(width >= 768px)" srcset="{{ project.root }}/images/map-768@1x.webp 1x, {{ project.root }}/images/map-768@2x.webp 2x" width="768" height="400">
		<img class="contacts__map" src="{{ project.root }}/images/map@1x.webp" srcset="{{ project.root }}/images/map@2x.webp 2x" width="320" height="362" alt="Карта">
	</picture>
</aside>
{% endmacro %}
